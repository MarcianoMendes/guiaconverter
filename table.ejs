<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela</title>
    <table>
        <thead>
            <% header.forEach(column=> { %>
                <th>
                    <%= column %>
                </th>
                <% }) %>
        </thead>
        <tbody>
            <% rows.forEach(row=> { %>
                <tr>
                    <% row.forEach(collumn=> { %>
                        <td>
                            <%= collumn %>
                        </td>
                        <% }) %>
                </tr>
                <% })%>
        </tbody>
    </table>
</head>

<body>

</body>

</html>